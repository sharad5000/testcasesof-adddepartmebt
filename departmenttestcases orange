#employment detail fill all the mandatory filed
    def test_006(self):
        from selenium import webdriver

        driver = webdriver.Chrome()
        driver.maximize_window()
        driver.get("https://testing.wenodo.com/")
        driver.find_element(By.XPATH,
                            "//div[@class='input-group mb-2']//input[@placeholder='E-mail Address']").send_keys(
            "mubashir@gmail.com")
        driver.find_element(By.XPATH, "//input[@id='password']").send_keys("demo123")
        driver.find_element(By.XPATH, "//login[@ng-show='LOGIN_FLAG']//button[@id='submit']").click()
        driver.implicitly_wait(5)
        # click on human Resource
        # click on human Resource
        driver.find_element(By.XPATH, "/html/body/div[2]/div[1]/div/div[2]/div[2]/div[2]/a/span").click()
        driver.implicitly_wait(5)
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[2]/div[1]/div[2]/div[1]/div[1]/button[1]").click()
        driver.implicitly_wait(5)
        # click on add position detail
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[2]/div[1]/div[2]/div[1]/div[1]/div[2]/a[2]").click()

        time.sleep(3)
        driver.implicitly_wait(2)

        # 2nd page position information

        # *position tittle
        driver.find_element(By.XPATH,
                            "/html/body/div[3]/div/section/div/ng-view/div[1]/form/div[1]/templateurl/div[1]/div[2]/div[1]/div/div/input").send_keys(
            "qaa123 automation team")

        # *department name
        driver.find_element(By.XPATH,
                            "/html/body/div[3]/div/section/div/ng-view/div[1]/form/div[1]/templateurl/div[1]/div[2]/div[2]/div/div/select").send_keys(
            "it and inovation")

        # reporting position
        driver.find_element(By.XPATH,
                            "/html/body/div[3]/div/section/div/ng-view/div[1]/form/div[1]/templateurl/div[1]/div[2]/div[3]/div/div/select").send_keys(
            "hr manager")

        # job description
        driver.find_element(By.XPATH,
                            "/html/body/div[3]/div/section/div/ng-view/div[1]/form/div[1]/templateurl/div[1]/div[2]/div[4]/div/div/textarea").send_keys(
            "management")
        time.sleep(10)
        for i in range(5):
            driver.find_element(By.XPATH, "/html").send_keys(Keys.PAGE_DOWN)
        time.sleep(5)
        driver.find_element(By.XPATH,
                            "/html/body/div[3]/div/section/div/ng-view/div[1]/form/div[6]/div/button[1]").click()
        time.sleep(10)
        # cost center code
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[2]/div[1]/form[1]/div[2]/templateurl[1]/div[1]/div[2]/div[1]/div[1]/div[1]/select[1]").send_keys(
            "it")
        # loction
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[2]/div[1]/form[1]/div[2]/templateurl[1]/div[1]/div[2]/div[2]/div[1]/div[1]/select[1]").send_keys(
            "london")

        time.sleep(3)
        # branch
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[2]/div[1]/form[1]/div[2]/templateurl[1]/div[1]/div[2]/div[3]/div[1]/div[1]/select[1]").send_keys(
            "london")

        # category
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[2]/div[1]/form[1]/div[2]/templateurl[1]/div[1]/div[2]/div[4]/div[1]/div[1]/select[1]").send_keys(
            "regular")
        time.sleep(10)
        # subcategory
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[2]/div[1]/form[1]/div[2]/templateurl[1]/div[1]/div[2]/div[5]/div[1]/div[1]/select[1]").send_keys(
            "full time")
        # manger position
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[2]/div[1]/form[1]/div[2]/templateurl[1]/div[1]/div[2]/div[6]/div[1]/div[1]/div[1]/div[1]/label[1]").click()
        # admin position
        # driver.find_element(By.XPATH, "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[2]/div[1]/form[1]/div[2]/templateurl[1]/div[2]/div[2]/div[1]/div[1]/div[1]/div[1]/div[1]/label[1]").click()
        for i in range(5):
            driver.find_element(By.XPATH, "/html").send_keys(Keys.PAGE_DOWN)
        time.sleep(10)

        # save and conitnue
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[2]/div[1]/form[1]/div[5]/div[1]/button[2]").click()
        time.sleep(5)

        if 'Add Position' in driver.find_element(By.XPATH,
                                                 "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[1]/div[1]/templateurl[1]/div[1]/h5[1]").text:

            assert True
        else:
            assert False
        driver.quit()




#leave the cost center code and submit the form
    def test_007(self):
        from selenium import webdriver

        driver = webdriver.Chrome()
        driver.maximize_window()
        driver.get("https://testing.wenodo.com/")
        driver.find_element(By.XPATH,
                            "//div[@class='input-group mb-2']//input[@placeholder='E-mail Address']").send_keys(
            "mubashir@gmail.com")
        driver.find_element(By.XPATH, "//input[@id='password']").send_keys("demo123")
        driver.find_element(By.XPATH, "//login[@ng-show='LOGIN_FLAG']//button[@id='submit']").click()
        driver.implicitly_wait(5)
        # click on human Resource
        # click on human Resource
        driver.find_element(By.XPATH, "/html/body/div[2]/div[1]/div/div[2]/div[2]/div[2]/a/span").click()
        driver.implicitly_wait(5)
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[2]/div[1]/div[2]/div[1]/div[1]/button[1]").click()
        driver.implicitly_wait(5)
        # click on add position detail
        driver.find_element(By.XPATH,
                            "/html[1]/body[1]/div[3]/div[1]/section[1]/div[1]/ng-view[1]/div[2]/div[1]/div[2]/div[1]/div[1]/div[2]/a[2]").click()

        time.sleep(3)
        driver.implicitly_wait(2)

        # 2nd page position information

        # *position tittle
        driver.find_element(By.XPATH,
                            "/html/body/div[3]/div/section/div/ng-view/div[1]/form/div[1]/templateurl/div[1]/div[2]/div[1]/div/div/input").send_keys(
            "ioni11team")

        # *department name
        driver.find_element(By.XPATH,
                            "/html/body/div[3]/div/section/div/ng-view/div[1]/form/div[1]/templateurl/div[1]/div[2]/div[2]/div/div/select").send_keys(
            "it and inovation")

        # reporting position
        driver.find_element(By.XPATH,
                            "/html/body/div[3]/div/section/div/ng-view/div[1]/form/div[1]/templateurl/div[1]/div[2]/div[3]/div/div/select").send_keys(
            "hr manager")

